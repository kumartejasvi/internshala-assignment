    "import csv\n",
    "import json\n",
    "infile=open(\"myjson.json\",\"r\")\n",
    "outfile=open(\"abc.csv\",\"w\")\n",
    "writer = csv.writer(outfile)\n",
    "for row in json.loads(infile.read()):\n",
    "    writer.writerow(row)\n",
    "outfile.close()"
